        echo 'Prperation 2nd Step'
      }
    }
    stage('Build') {
      steps {
        echo 'Build First Step'
        echo 'Build Step-2'
      }
    }
    stage('Test') {
      steps {
        echo 'Test1'
      }
    }
    stage('Deploye') {
      steps {
        echo 'Deploye Msg1'
      }
    }
  }
}